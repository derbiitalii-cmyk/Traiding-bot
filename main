python
import asyncio
import ccxt
import pandas as pd
from aiogram import Bot, Dispatcher, types
from aiogram.filters import Command

# –ù–∞—Å—Ç—Ä–æ–π–∫–∏
API_TOKEN = '7678882216:AAEhNSpyWVI1f6Cj004jKm97QTZ6FA5UucE'
SYMBOL = 'BTC/USDT'
TIMEFRAME = '1h'

bot = Bot(token=API_TOKEN)
dp = Dispatcher()
exchange = ccxt.binance()

def get_levels(df):
    """
    –ü—Ä–æ—Å—Ç–µ–π—à–∏–π –ø–æ–∏—Å–∫ —É—Ä–æ–≤–Ω–µ–π –ø–æ –ì–µ—Ä—á–∏–∫—É: 
    –º–∏–Ω–∏–º—É–º/–º–∞–∫—Å–∏–º—É–º, –∫–æ—Ç–æ—Ä—ã–µ –∫–∞—Å–∞–ª–∏—Å—å –Ω–µ—Å–∫–æ–ª—å–∫–æ —Ä–∞–∑.
    """
    # –ë–µ—Ä–µ–º –º–∞–∫—Å–∏–º—É–º—ã –ø–æ—Å–ª–µ–¥–Ω–∏—Ö 50 —Å–≤–µ—á–µ–π
    levels = df['high'].tail(50).value_counts()
    # –£—Ä–æ–≤–µ–Ω—å, –∫–æ—Ç–æ—Ä—ã–π –≤—Å—Ç—Ä–µ—á–∞–ª—Å—è –±–æ–ª–µ–µ 1 —Ä–∞–∑–∞ (–ø–æ–¥—Ç–≤–µ—Ä–∂–¥–µ–Ω–Ω—ã–π)
    key_levels = levels[levels > 1].index.tolist()
    return key_levels

def analyze_breakout(df):
    """
    –õ–æ–≥–∏–∫–∞ –∞–Ω–∞–ª–∏–∑–∞ –ø—Ä–æ–±–æ—è:
    1. –ë–ª–∏–∂–Ω–∏–π —Ä–µ—Ç–µ—Å—Ç (—Ü–µ–Ω–∞ –ø–æ–¥–æ—à–ª–∞ –∫ —É—Ä–æ–≤–Ω—é –∏ —Å—Ç–æ–∏—Ç —Ä—è–¥–æ–º)
    2. –ó–∞—Ç—É—Ö–∞–Ω–∏–µ –≤–æ–ª–∞—Ç–∏–ª—å–Ω–æ—Å—Ç–∏ (–ê–¢–† —É–º–µ–Ω—å—à–∞–µ—Ç—Å—è)
    """
    current_price = df['close'].iloc[-1]
    highs = df['high'].tail(20)
    lows = df['low'].tail(20)
    
    # –ò—â–µ–º –ª–æ–∫–∞–ª—å–Ω–æ–µ —Å–æ–ø—Ä–æ—Ç–∏–≤–ª–µ–Ω–∏–µ
    resistance = highs.max()
    
    # –£—Å–ª–æ–≤–∏–µ "–ü–æ–¥–∂–∞—Ç–∏—è": –∫–∞–∂–¥—ã–π –ø–æ—Å–ª–µ–¥—É—é—â–∏–π –º–∏–Ω–∏–º—É–º –≤—ã—à–µ –ø—Ä–µ–¥—ã–¥—É—â–µ–≥–æ
    is_squeeze = df['low'].iloc[-3] < df['low'].iloc[-2] < df['low'].iloc[-1]
    
    # –†–∞—Å—Å—Ç–æ—è–Ω–∏–µ –¥–æ —É—Ä–æ–≤–Ω—è (–±–ª–∏–∑–æ—Å—Ç—å)
    distance = (resistance - current_price) / current_price
    
    if distance < 0.005 and is_squeeze:
        return f"üöÄ –°–∏–≥–Ω–∞–ª –Ω–∞ –ü–†–û–ë–û–ô –≤–≤–µ—Ä—Ö!\n–£—Ä–æ–≤–µ–Ω—å: {resistance}\n–¶–µ–Ω–∞: {current_price}\n–°—Ç–∞—Ç—É—Å: –ü–æ–¥–∂–∞—Ç–∏–µ —Å—Ñ–æ—Ä–º–∏—Ä–æ–≤–∞–Ω–æ."
    return "–ê–Ω–∞–ª–∏–∑ –∑–∞–≤–µ—Ä—à–µ–Ω: –ß–µ—Ç–∫–∏—Ö –ø–∞—Ç—Ç–µ—Ä–Ω–æ–≤ –ø–æ –ì–µ—Ä—á–∏–∫—É –Ω–µ –Ω–∞–π–¥–µ–Ω–æ."

@dp.message(Command("start"))
async def start(message: types.Message):
    await message.answer("–ë–æ—Ç –∑–∞–ø—É—â–µ–Ω. –ù–∞–ø–∏—à–∏ /analyze –¥–ª—è –ø–æ–∏—Å–∫–∞ –ø—Ä–æ–±–æ—è –ø–æ BTC/USDT.")

@dp.message(Command("analyze"))
async def analyze(message: types.Message):
    await message.answer("–ó–∞–≥—Ä—É–∂–∞—é –¥–∞–Ω–Ω—ã–µ –∏ –∏—â—É —É—Ä–æ–≤–Ω–∏...")
    
    # –ü–æ–ª—É—á–∞–µ–º —Å–≤–µ—á–∏
    ohlcv = exchange.fetch_ohlcv(SYMBOL, timeframe=TIMEFRAME, limit=100)
    df = pd.DataFrame(ohlcv, columns=['timestamp', 'open', 'high', 'low', 'close', 'volume'])
    
    result = analyze_breakout(df)
    await message.answer(result)

async def main():
    await dp.start_polling(bot)

if __name__ == "__main__":
    asyncio.run(main())
